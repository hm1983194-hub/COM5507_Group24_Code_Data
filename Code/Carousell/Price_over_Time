   ],
   "source": [
    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "from datetime import datetime, timedelta\n",
    "import re\n",
    "import matplotlib\n",
    "\n",
    "# 设置中文字体（如果需要显示中文可以保留，如果只显示英文可以移除）\n",
    "plt.rcParams['font.sans-serif'] = ['SimHei']  # 用来正常显示中文标签\n",
    "plt.rcParams['axes.unicode_minus'] = False    # 用来正常显示负号\n",
    "\n",
    "def convert_relative_time(time_str, base_date=datetime(2025, 11, 23)):\n",
    "    \"\"\"\n",
    "    将相对时间转换为绝对日期\n",
    "    格式：1分鐘前、1 小時前、1天前\n",
    "    \"\"\"\n",
    "    # 清理字符串中的空格和特殊字符\n",
    "    time_str = str(time_str).strip()\n",
    "    \n",
    "    # 匹配模式：数字 + 单位（分鐘、小時、天）\n",
    "    pattern = r'(\\d+)\\s*(分鐘|小時|天)前'\n",
    "    match = re.search(pattern, time_str)\n",
    "    \n",
    "    if not match:\n",
    "        return base_date.strftime(\"%m.%d\")\n",
    "    \n",
    "    number = int(match.group(1))\n",
    "    unit = match.group(2)\n",
    "    \n",
    "    # 根据单位计算时间差\n",
    "    if unit == '分鐘':\n",
    "        delta = timedelta(minutes=number)\n",
    "    elif unit == '小時':\n",
    "        delta = timedelta(hours=number)\n",
    "    elif unit == '天':\n",
    "        delta = timedelta(days=number)\n",
    "    else:\n",
    "        return base_date.strftime(\"%m.%d\")\n",
    "    \n",
    "    # 计算绝对日期\n",
    "    absolute_date = base_date - delta\n",
    "    return absolute_date.strftime(\"%m.%d\")\n",
    "\n",
    "def clean_price(price_str):\n",
    "    \"\"\"\n",
    "    清理价格字符串，转换为数值\n",
    "    \"\"\"\n",
    "    if pd.isna(price_str):\n",
    "        return np.nan\n",
    "    \n",
    "    price_str = str(price_str)\n",
    "    # 移除货币符号和空格\n",
    "    price_str = re.sub(r'[^\\d.]', '', price_str)\n",
    "    \n",
    "    try:\n",
    "        return float(price_str)\n",
    "    except:\n",
    "        return np.nan\n",
    "\n",
    "def date_to_sortable(date_str):\n",
    "    \"\"\"将MM.DD格式转换为可排序的数值\"\"\"\n",
    "    try:\n",
    "        month, day = date_str.split('.')\n",
    "        return int(month) * 100 + int(day)\n",
    "    except:\n",
    "        return 0\n",
    "\n",
    "# 读取数据\n",
    "df = pd.read_csv('D:\\AImportant\\留学\\港城\\course\\Sem A\\COM5507 Social Media Data\\Group Project\\Data\\cleaned_sweetbean.csv') \n",
    "\n",
    "print(\"Data Information:\")\n",
    "print(f\"Data shape: {df.shape}\")\n",
    "print(f\"Column names: {df.columns.tolist()}\")\n",
    "print(\"\\nFirst 5 rows:\")\n",
    "print(df.head())\n",
    "\n",
    "# 数据预处理\n",
    "print(\"\\nData preprocessing...\")\n",
    "\n",
    "# 1. 转换时间格式\n",
    "print(\"Converting time format...\")\n",
    "df['绝对日期'] = df['发布时间'].apply(convert_relative_time)\n",
    "print(f\"Time conversion completed, unique dates: {df['绝对日期'].unique()}\")\n",
    "\n",
    "# 2. 清理价格数据\n",
    "print(\"Cleaning price data...\")\n",
    "df['价格_数值'] = df['价格'].apply(clean_price)\n",
    "print(f\"Price cleaning completed, valid prices: {df['价格_数值'].notna().sum()}\")\n",
    "\n",
    "# 移除无效数据\n",
    "original_count = len(df)\n",
    "df = df.dropna(subset=['价格_数值', '绝对日期'])\n",
    "print(f\"After removing invalid data: {len(df)} rows (original: {original_count} rows)\")\n",
    "\n",
    "# 按日期分组计算价格中位数\n",
    "print(\"\\nCalculating grouped statistics...\")\n",
    "grouped_data = df.groupby('绝对日期')['价格_数值'].median().reset_index()\n",
    "\n",
    "# 按日期排序 - 修复排序方法\n",
    "grouped_data['sort_key'] = grouped_data['绝对日期'].apply(date_to_sortable)\n",
    "grouped_data_sorted = grouped_data.sort_values('sort_key').drop('sort_key', axis=1)\n",
    "\n",
    "print(\"\\nPreparing to plot...\")\n",
    "\n",
    "# 创建图表\n",
    "plt.figure(figsize=(14, 8))\n",
    "\n",
    "# 绘制柱状图\n",
    "bars = plt.bar(range(len(grouped_data_sorted)), \n",
    "               grouped_data_sorted['价格_数值'], \n",
    "               color='#4ECDC4', alpha=0.8)\n",
    "\n",
    "plt.xlabel('Post Date', fontsize=14, fontweight='bold')\n",
    "plt.ylabel('Median Price (CNY)', fontsize=14, fontweight='bold')\n",
    "plt.title('Sweetbean Products Median Price Over Time', fontsize=16, fontweight='bold')\n",
    "\n",
    "# 设置x轴标签\n",
    "plt.xticks(range(len(grouped_data_sorted)), grouped_data_sorted['绝对日期'], rotation=45)\n",
    "\n",
    "# 在柱子上显示数值\n",
    "for i, bar in enumerate(bars):\n",
    "    height = bar.get_height()\n",
    "    plt.text(bar.get_x() + bar.get_width()/2., height + 0.1,\n",
    "             f'{height:.1f}', ha='center', va='bottom', fontsize=9)\n",
    "\n",
    "plt.grid(axis='y', alpha=0.3)\n",
    "plt.tight_layout()\n",
    "\n",
    "# 显示图表\n",
    "plt.show()\n",
    "\n",
    "# 保存图表\n",
    "plt.savefig('dimoo_price_analysis.png', dpi=300, bbox_inches='tight')\n",
    "print(\"Chart saved as 'dimoo_price_analysis.png'\")\n",
    "\n",
    "# 输出一些统计信息\n",
    "print(\"\\n=== Statistical Summary ===\")\n",
    "print(f\"Total data points: {len(df)}\")\n",
    "print(f\"Time range: {grouped_data_sorted['绝对日期'].iloc[0]} to {grouped_data_sorted['绝对日期'].iloc[-1]}\")\n",
    "print(f\"Number of dates: {len(grouped_data_sorted)}\")\n",
    "\n",
    "# 输出每个日期的详细统计\n",
    "print(\"\\nPrice statistics by date:\")\n",
    "for _, row in grouped_data_sorted.iterrows():\n",
    "    date_data = df[df['绝对日期'] == row['绝对日期']]\n",
    "    print(f\"{row['绝对日期']}: Median={row['价格_数值']:.2f} CNY, Products={len(date_data)}\")\n",
    "\n",
    "# 修复排序问题 - 使用新的方法排序日期计数\n",
    "print(\"\\nProduct count by date:\")\n",
    "date_counts = df['绝对日期'].value_counts().reset_index()\n",
    "date_counts.columns = ['Date', 'Count']\n",
    "date_counts['sort_key'] = date_counts['Date'].apply(date_to_sortable)\n",
    "date_counts_sorted = date_counts.sort_values('sort_key').drop('sort_key', axis=1)\n",
    "print(date_counts_sorted.to_string(index=False))"
   ]
  },
