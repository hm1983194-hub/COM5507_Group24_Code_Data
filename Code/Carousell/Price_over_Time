    "    except:\n",
    "        return 0\n",
    "\n",
    "# 读取数据\n",
    "df = pd.read_csv('D:\\AImportant\\留学\\港城\\course\\Sem A\\COM5507 Social Media Data\\Group Project\\Data\\cleaned_skullpanda.csv') \n",
