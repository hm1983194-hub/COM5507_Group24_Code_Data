    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "from datetime import datetime, timedelta\n",
    "import re\n",
    "import matplotlib\n",
    "\n",
    "# 设置中文字体（如果需要显示中文可以保留，如果只显示英文可以移除）\n",
    "plt.rcParams['font.sans-serif'] = ['SimHei']  # 用来正常显示中文标签\n",
    "plt.rcParams['axes.unicode_minus'] = False    # 用来正常显示负号\n",
    "\n",
    "def convert_relative_time(time_str, base_date=datetime(2025, 11, 23)):\n",
    "    \"\"\"\n",
    "    将相对时间转换为绝对日期\n",
    "    格式：1分鐘前、1 小時前、1天前\n",
    "    \"\"\"\n",
    "    # 清理字符串中的空格和特殊字符\n",
    "    time_str = str(time_str).strip()\n",
    "    \n",
    "    # 匹配模式：数字 + 单位（分鐘、小時、天）\n",
    "    pattern = r'(\\d+)\\s*(分鐘|小時|天)前'\n",
    "    match = re.search(pattern, time_str)\n",
    "    \n",
    "    if not match:\n",
    "        return base_date.strftime(\"%m.%d\")\n",
    "    \n",
    "    number = int(match.group(1))\n",
    "    unit = match.group(2)\n",
    "    \n",
    "    # 根据单位计算时间差\n",
    "    if unit == '分鐘':\n",
    "        delta = timedelta(minutes=number)\n",
    "    elif unit == '小時':\n",
    "        delta = timedelta(hours=number)\n",
    "    elif unit == '天':\n",
    "        delta = timedelta(days=number)\n",
    "    else:\n",
    "        return base_date.strftime(\"%m.%d\")\n",
    "    \n",
    "    # 计算绝对日期\n",
    "    absolute_date = base_date - delta\n",
    "    return absolute_date.strftime(\"%m.%d\")\n",
    "\n",
    "def clean_price(price_str):\n",
    "    \"\"\"\n",
    "    清理价格字符串，转换为数值\n",
    "    \"\"\"\n",
    "    if pd.isna(price_str):\n",
    "        return np.nan\n",
    "    \n",
    "    price_str = str(price_str)\n",
    "    # 移除货币符号和空格\n",
    "    price_str = re.sub(r'[^\\d.]', '', price_str)\n",
